{
  "name": "imgix-core-js",
  "description": "Core imgix URL and srcset builder library.",
  "version": "2.3.2",
  "repository": "https://github.com/imgix/imgix-core-js",
  "license": "BSD-2-Clause",
  "main": "dist/imgix-core-js.cjs.js",
  "module": "dist/imgix-core-js.esm.js",
  "browser": "dist/imgix-core-js.umd.js",
  "type": "module",
  "types": "dist/imgix-core-js.d.ts",
  "dependencies": {
    "js-base64": "~2.6.0",
    "md5": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.1",
    "benchmark": "2.1.4",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.4",
    "sinon": "9.2.3",
    "typescript": "4.1.3",
    "uglify-js": "3.12.4"
  },
  "scripts": {
    "build": "rollup -c",
    "compile": "cp ./src/imgix-core-js.d.ts ./dist/imgix-core-js.d.ts && tsc",
    "dev": "rollup -c -w",
    "prepublish": "npm run build && npm run compile",
    "pretest": "npm run build",
    "pretty": "prettier --write '{src,test}/**/*.js'",
    "test": "mocha ./test/build/*.cjs",
    "test:performance": "mocha test/performance/*.js"
  },
  "files": [
    "dist",
    "README.md",
    "src"
  ]
}
